{
  "name": "nuxt-starter",
  "private": true,
  "type": "module",
  "description": "React Starter",
  "author": "Kabutoa <yinyun957@163.com>",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:open": "nuxt dev -o",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "nuxi --version && nuxi typecheck",
    "lint:check": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --fix --cache",
    "format:write": "prettier --write \"**/*.{vue,ts,tsx,js,jsx,mjs,mts,cjs,cts,mdx,md,css,json,html,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{vue,ts,tsx,js,jsx,mjs,mts,cjs,cts,mdx,md,css,json,html,yaml,yml}\""
  },
  "dependencies": {
    "nuxt": "^3.12.4",
    "vue": "latest"
  },
  "packageManager": "pnpm@9.1.3+sha512.7c2ea089e1a6af306409c4fc8c4f0897bdac32b772016196c469d9428f1fe2d5a21daf8ad6512762654ac645b5d9136bb210ec9a00afa8dbc4677843ba362ecd",
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "vite-plugin-eslint2": "^4.4.0"
  }
}
